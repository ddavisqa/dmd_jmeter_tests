function fixIdent(idname,quote,quoteChar,caseflag) {

     var id;
	switch (caseflag) {

		case 0:  // leave alone
		    id = idname;
		    break;
		case 1: // upcase
		    id = idname.toUpperCase();
		    break;
	     case -1: // downcase
	         id = idname.toLowerCase();
	         break;
	     default:
	         id = idname;
	         break;
	     
	}

	if (quote) {
		id = quoteChar + id + quoteChar;
	}
	return id;
}

